# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /								controllers.FrontController.index()
GET     /:x								controllers.FrontController.indexWithRoute(x: String)

# Rest API
GET		/helloworld/					controllers.RestAPIController.helloWorld()
GET		/getAllImages/					controllers.RestAPIController.getAllImages()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file					controllers.Assets.at(path="/public", file)
